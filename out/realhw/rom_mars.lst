(2)    9/  945710 :                     ; INTRUMENT/PATCH FORMAT:
(2)   10/  945710 :                     ; dc.b $30,$34,$38,$3C	; Your FM registers following this order
(2)   11/  945710 :                     ; dc.b $40,$44,$48,$4C	; **
(2)   12/  945710 :                     ; dc.b $50,$54,$58,$5C	; **
(2)   13/  945710 :                     ; dc.b $60,$64,$68,$6C	; **
(2)   14/  945710 :                     ; dc.b $70,$74,$78,$7C	; **
(2)   15/  945710 :                     ; dc.b $80,$84,$88,$8C	; **
(2)   16/  945710 :                     ; dc.b $90,$94,$98,$9C	; **
(2)   17/  945710 :                     ; dc.b $B0,$B4,$22,$28	; **
(2)   18/  945710 :                     ; dc.w OP1,OP2,OP3,OP4	; OPTIONAL: Manual FM3 frequencies
(2)   19/  945710 :                     ;
(2)   20/  945710 :                     ; Notes:
(2)   21/  945710 :                     ; SSG-EG CAN be used, but can get problematic on non-genuine systems.
(2)   22/  945710 :                     ;
(2)   23/  945710 :                     ; $B4 - AMS/PMS: %00aa0ppp
(2)   24/  945710 :                     ; a | AMS
(2)   25/  945710 :                     ; p | PMS
(2)   26/  945710 :                     ; Keep panning bits 0, set the panning manually in your track.
(2)   27/  945710 :                     ;
(2)   28/  945710 :                     ; $22 - LFO: %0000evvv
(2)   29/  945710 :                     ; e | Enable
(2)   30/  945710 :                     ; v | Value
(2)   31/  945710 :                     ; This is a global setting, this will affect ALL sound.
(2)   32/  945710 :                     ;
(2)   33/  945710 :                     ; $28 - KEYS: %oooo0000
(2)   34/  945710 :                     ; o | Operators 4-1
(2)   35/  945710 :                     ;
(2)   36/  945710 :                     ; For making your own FM patches:
(2)   37/  945710 :                     ; Run FM_EDITOR.bin on an emulator and
